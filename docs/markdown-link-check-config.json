{
  "timeout": "20s",
  "retryOn429": true,
  "retryCount": 3,
  "fallbackRetryDelay": "30s",
  "aliveStatusCodes": [200, 206],
  "ignorePatterns": [
    {
      "pattern": "http://www.aibio.center"
    },
    {
      "pattern": "https://docs.solapi.com"
    },
    {
      "pattern": "http://localhost"
    },
    {
      "pattern": "https://localhost"
    },
    {
      "pattern": "127.0.0.1"
    },
    {
      "pattern": "^file://"
    },
    {
      "pattern": "^mailto:"
    },
    {
      "pattern": "center-production.*\\.up\\.railway\\.app"
    },
    {
      "pattern": "center.*\\.vercel\\.app"
    },
    {
      "pattern": "supabase\\.co/dashboard"
    },
    {
      "pattern": "github\\.com.*\\.git"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^../",
      "replacement": "{{BASEURL}}/"
    }
  ],
  "httpHeaders": [
    {
      "urls": ["https://github.com"],
      "headers": {
        "Accept": "text/html",
        "User-Agent": "Mozilla/5.0 (compatible; link-checker)"
      }
    }
  ]
}