{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --config vite.config.ts",
    "dev:stable": "vite --config vite.config.ts",
    "dev:fast": "vite --config vite.config.alternative.ts",
    "dev:no-ws": "vite --config vite.config.no-ws.ts",
    "dev:monitor": "npm run dev:stable & python ../scripts/auto_console_monitor.py",
    "prebuild": "grep -R \"http://center-production\" src && { echo '❌ HTTP URL detected in source'; exit 1; } || echo '✅ No HTTP URLs in source'",
    "build": "vite build",
    "predeploy": "cd .. && python scripts/pre_deploy_check.py",
    "deploy": "npm run predeploy && npm run build",
    "build-force": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "generate:types": "node scripts/generate-types.js",
    "update:types": "cd ../backend && python scripts/generate_openapi.py && cd ../frontend && npm run generate:types"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.4",
    "@supabase/supabase-js": "^2.50.0",
    "@tanstack/react-query": "^5.80.2",
    "axios": "^1.9.0",
    "date-fns": "^4.1.0",
    "jwt-decode": "^4.0.0",
    "lucide-react": "^0.512.0",
    "playwright": "^1.53.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hot-toast": "^2.5.2",
    "react-router-dom": "^7.6.2",
    "recharts": "^2.15.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/node": "^24.0.1",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "glob": "^11.0.3",
    "globals": "^16.0.0",
    "openapi-typescript": "^7.8.0",
    "postcss": "^8.5.6",
    "rollup-plugin-visualizer": "^6.0.3",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5"
  }
}
